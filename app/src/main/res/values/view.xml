<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomMenu">
        //选中文字颜色
        <attr name="checkTv_true" format="color"></attr>
        //未选中文字颜色
        <attr name="checkTv_flase" format="color"></attr>
        //选中图片
        <attr name="checkImg_true" format="reference"></attr>
        //为选中图片
        <attr name="checkImg_flase" format="reference"></attr>
        //为选中图片
        <attr name="Tvsize" format="dimension"></attr>
        //是否开启动画
        <attr name="anmiable" format="boolean"></attr>
        //设置初始值
        <attr name="text" format="string"></attr>
    </declare-styleable>

    <declare-styleable name="BottomLayout">
        <attr name="checkPosition" format="integer"></attr>
    </declare-styleable>
</resources>